{
  "name": "fslp",
  "version": "0.6.0",
  "description": "Finite State Language reference parser",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava src/js/tests/*.js",
    "test_verbose": "ava src/js/tests/*.js -v",
    "make_peg": "rm -f src/js/fsl-peg.pegjs && concat -o src/js/fsl-peg.pegjs src/peg/preamble.pegjs src/peg/doc_structure.pegjs src/peg/label.pegjs src/peg/licenses.pegjs src/peg/numbers.pegjs src/peg/semver.pegjs src/peg/named_color.pegjs src/peg/color.pegjs src/peg/arrow.pegjs src/peg/shape.pegjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/fslp.git"
  },
  "keywords": [
    "fsl",
    "fslp",
    "parser",
    "fsl-parser",
    "fsl_parser",
    "fslparser"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/fslp/issues"
  },
  "homepage": "https://github.com/StoneCypher/fslp#readme",
  "devDependencies": {
    "ava": "^0.23.0",
    "ava-spec": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "concat": "^1.0.3",
    "eslint": "^4.10.0",
    "eslint-config-stonecypher": "^1.15.7",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-jsdoc": "^3.1.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-new-with-error": "^1.1.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-unicorn": "^2.1.2",
    "nyc": "^11.2.1"
  },
  "dependencies": {}
}
